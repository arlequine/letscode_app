<p id="notice"><%= notice %></p>

<dl class="dl-horizontal">
	<dt>Project Name:</dt>
	<dd><%= @proyecto.title %></dd>
	<dt>Project Description:</dt>
	<dd><%= @proyecto.description %></dd>
	<dt>Incentive:</dt>
	<dd><%= @proyecto.incentive %></dd>
	<dt>Result:</dt>
	<dd><%= @proyecto.result %></dd>
	<dt>Number of Students Accepted:</dt>
	<dd><%= @proyecto.num_students %></dd>
	<dt>Project Deadline:</dt>
	<dd><%= @proyecto.dead_line %></dd>
	<dt>Skills requeridos:</dt>
	<% @proyecto.skills.each do |skill| %>
	<dd><%= skill.name %></dd>
	<% end %>
</dl>


