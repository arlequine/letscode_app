<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
	<thead>
		<tr>
			<th>Project Title</th>
			<th>Project Description</th>
			<th>Incentive</th>
			<th>Project Result</th>
			<th>Students Accepted</th>
			<th>Project Deadline</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @proyectos.each do |proyecto| %>
		  <tr id="<%= "delete_proyecto_#{proyecto.id}" %>">
		  	<td><%= proyecto.title %></td>
		  	<td><%= proyecto.description %></td>
		  	<td><%= proyecto.incentive %></td>
		  	<td><%= proyecto.result %></td>
		  	<td><%= proyecto.num_students %></td>
		  	<td><%= proyecto.dead_line %></td>
		  	<td><%= link_to 'Show', proyecto %></td>
		  	<td><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
		  	<td><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' }, :remote => true %></td>
		  </tr>
		<% end %>
	</tbody>
</table>

<br>

<%= link_to 'New Project', new_proyecto_path %>