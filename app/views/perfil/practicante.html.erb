<p id="notice"><%= notice %></p>

<dl class="dl-horizontal">
	<dt>Name:</dt>
	<dd><%= @user.name %></dd>
	<dt>Last name:</dt>
	<dd><%= @user.last_name %></dd>
	<dt>gender:</dt>
	<dd><%= @user.genre %></dd>
	<dt>email:</dt>
	<dd><%= @user.email %></dd>
	<dt>Birthyear:</dt>
	<dd><%= @user.birthday_year %></dd>
	<dt>spects:</dt>
	<dd><%= @user.opinion %></dd>
	<dt>Skills:</dt>
	<% @user.skills.each do |skill| %>
	<dd><%= skill.name %></dd>
	<% end %> 
</dl>